{
	"name": "icampana-blog2",
	"version": "1.1.0",
	"private": true,
	"scripts": {
		"dev": "tinacms dev -c \"next dev --turbopack\"",
		"build": "tinacms build && pnpm build:content && pnpm build:index && next build",
		"build:admin-css": "npx tailwindcss build -i styles/globals.css -o public/admin.css",
		"build:content": "contentlayer2 build --verbose",
		"build:index": "node lib/search-index.js",
		"export": "pnpm build:admin-css && next build && next export",
		"start": "tinacms build && next start",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format .",
		"format:write": "biome format --write .",
		"postbuild": "next-sitemap",
		"clean": "npx rimraf .contentlayer .next .turbo out"
	},
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^7.0.1",
		"@fortawesome/free-brands-svg-icons": "^7.0.1",
		"@fortawesome/free-solid-svg-icons": "^7.0.1",
		"@fortawesome/react-fontawesome": "^3.0.2",
		"@remark-embedder/core": "^3.0.3",
		"@remark-embedder/transformer-oembed": "^5.0.1",
		"@tailwindcss/postcss": "^4.1.13",
		"@tinacms/cli": "^1.10.3",
		"contentlayer2": "^0.5.8",
		"date-fns": "^4.1.0",
		"fuse.js": "^7.1.0",
		"html-react-parser": "^5.2.6",
		"marked": "^16.3.0",
		"next": "^15.5.3",
		"next-contentlayer2": "^0.5.8",
		"next-seo": "^6.8.0",
		"next-sitemap": "^4.2.3",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"reading-time": "^1.5.0",
		"rehype-highlight": "^7.0.2",
		"rehype-slug": "^6.0.0",
		"rehype-stringify": "^10.0.1",
		"remark": "^15.0.1",
		"remark-breaks": "^4.0.0",
		"remark-directive": "^3.0.0",
		"remark-emoji": "^5.0.2",
		"remark-frontmatter": "^5.0.0",
		"remark-gfm": "^4.0.1",
		"remark-html": "^16.0.1",
		"remark-parse": "^11.0.0",
		"remark-rehype": "^11.1.2",
		"striptags": "^3.2.0",
		"theme-change": "^2.5.0",
		"tinacms": "^2.8.3"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.4",
		"@types/marked": "^6.0.0",
		"@types/node": "^24.5.2",
		"@types/react": "^19.1.13",
		"@types/react-dom": "^19.1.9",
		"autoprefixer": "^10.4.21",
		"daisyui": "^5.1.13",
		"postcss": "^8.5.6",
		"tailwindcss": "^4.1.13",
		"typescript": "^5.9.2"
	}
}

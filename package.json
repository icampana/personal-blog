{
  "name": "icampana-blog2",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "dev": "tinacms dev -c \"next dev\"",
    "build": "tinacms build && contentlayer build && next build",
    "build:admin-css": "npx tailwindcss build -i styles/globals.css -o public/admin.css",
    "build:content": "contentlayer build --verbose",
    "export": "yarn build:admin-css && next build && next export",
    "start": "tinacms build && next start",
    "lint": "next lint",
    "postbuild": "next-sitemap",
    "clean": "npx rimraf .markdownlayer .next .turbo out"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-brands-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@next/eslint-plugin-next": "^14.2.3",
    "@remark-embedder/core": "^3.0.2",
    "@remark-embedder/transformer-oembed": "^3.0.0",
    "@tinacms/cli": "^1.5.45",
    "contentlayer2": "^0.4.6",
    "date-fns": "^3.6.0",
    "fuse.js": "^7.0.0",
    "marked": "^12.0.2",
    "next": "^14.2.3",
    "next-contentlayer": "^0.3.4",
    "next-seo": "^6.5.0",
    "next-sitemap": "^4.2.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "reading-time": "^1.5.0",

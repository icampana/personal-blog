{
  "name": "icampana-blog2",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "dev": "tinacms dev -c \"next dev --turbopack\"",
    "build": "tinacms build && yarn build:content && yarn build:index && next build",
    "build:admin-css": "npx tailwindcss build -i styles/globals.css -o public/admin.css",
    "build:content": "contentlayer2 build --verbose",
    "build:index": "node lib/search-index.js",
    "export": "yarn build:admin-css && next build && next export",
    "start": "tinacms build && next start",
    "lint": "next lint",
    "postbuild": "next-sitemap",
    "clean": "npx rimraf .contentlayer .next .turbo out"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.0.0",
    "@fortawesome/free-brands-svg-icons": "^7.0.0",
    "@fortawesome/free-solid-svg-icons": "^7.0.0",
    "@fortawesome/react-fontawesome": "^0.2.3",
    "@remark-embedder/core": "^3.0.3",
    "@remark-embedder/transformer-oembed": "^5.0.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@tinacms/cli": "^1.10.1",
    "contentlayer2": "^0.5.8",
    "date-fns": "^4.1.0",
    "fuse.js": "^7.1.0",
    "marked": "^16.1.2",
    "next": "^15.4.5",
    "next-contentlayer2": "^0.5.8",
    "next-seo": "^6.8.0",
    "next-sitemap": "^4.2.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "reading-time": "^1.5.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "remark": "^15.0.1",
    "remark-breaks": "^4.0.0",
    "remark-emoji": "^5.0.1",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "remark-html": "^16.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "striptags": "^3.2.0",
    "theme-change": "^2.5.0",
    "tinacms": "^2.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@next/eslint-plugin-next": "^15.4.5",
    "@types/marked": "^6.0.0",
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "autoprefixer": "^10.4.21",

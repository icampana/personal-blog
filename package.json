    "striptags": "^3.2.0",
    "tinacms": "^1.6.3"
  },
  "devDependencies": {
    "@types/marked": "^6.0.0",
    "@types/node": "^20.12.8",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.1.1",
    "eslint-config-next": "^14.2.3",
